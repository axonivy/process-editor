      "postDebugTask": "Terminate All Tasks"
    },
    {
      "name": "Launch Inscription (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3003?server=localhost:8080/~Developer-inscription-test-project&app=Developer-inscription-test-project&pmv=inscription-test-project&pid=16C51F1EE34D5EF5-f15",
